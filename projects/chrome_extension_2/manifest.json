{
  "manifest_version": 2,
  "name": "Shipping_Policy",
  "version": "0.1",

  "permissions":[
    "activeTab",
    "tabs"
  ],

  "background": {
  "scripts": ["background_2502.js"]
                  },


  "content_scripts": [
  {
    "matches": [
      "http://*/*", "https://*/*"
    ],

    "js": ["jquery-3.4.1.min.js", "content_2502.js"],

    "css": ["styles.css"]
  }
   ],

  "browser_action": {
    "default_icon": "icon.png"
	}
}